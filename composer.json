{
    "name": "projet-normandie/message-bundle",
    "description": "Normandie Message bundle",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Benard David",
            "email": "magicbart@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "api-platform/core": "^2.7",
        "doctrine/orm": "^2.13",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "knplabs/doctrine-behaviors": "~2.0",
        "symfony/console": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/security-bundle": "^5.4"
    },
    "suggest": {
        "sonata-project/admin-bundle": "To use administration with sonata",
        "sonata-project/doctrine-orm-admin-bundle": "To use doctrine orm into sonata admin"
    },
    "autoload": {
        "psr-4": { "ProjetNormandie\\MessageBundle\\": "src/" }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
