{
  "name": "projet-normandie/message-bundle",
  "description": "Normandie Message bundle",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Benard David",
      "email": "magicbart@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "api-platform/api-pack": "^1.2",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "~2.0",
    "symfony/validator": "^4.4 || ^5.1",
    "symfony/framework-bundle": "^4.4 || ^5.1",
    "knplabs/doctrine-behaviors": "~2.0"
  },
  "suggest": {
    "sonata-project/admin-bundle": "To use administration with sonata",
    "a2lix/translation-form-bundle": "To use translation in administration with sonata admin"
  },
  "autoload": {
    "psr-4": { "ProjetNormandie\\MessageBundle\\": "" }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
